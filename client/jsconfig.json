{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$lib": [
        "src/lib"
      ],
      "$lib/*": [
        "src/lib/*"
      ],
      "$routes": [
        "src/routes"
      ],
      "$routes/*": [
        "src/routes/*"
      ],
      "$functions": [
        "src/functions"
      ],
      "$functions/*": [
        "src/functions/*"
      ]
    }
  },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.js",
    "src/**/*.svelte"
  ]
}
